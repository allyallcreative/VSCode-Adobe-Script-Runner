{
	"env": {
		"browser": false,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"sourceType": "module"
	},
	"rules": {
		"no-unreachable": "warn",
		"no-unused-vars": "warn",
		"semi": "warn",
		"valid-typeof": "warn"
	}
}
